{
  "kind": "collectionType",
  "collectionName": "customer_messages",
  "info": {
    "singularName": "customer-message",
    "pluralName": "customer-messages",
    "displayName": "customer-message",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "contact",
        "feedback"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "replied",
        "unanswered",
        "delete"
      ]
    },
    "rating": {
      "type": "enumeration",
      "enum": [
        "best",
        "normal",
        "wrong"
      ]
    },
    "message": {
      "type": "text",
      "required": true
    }
  }
}
