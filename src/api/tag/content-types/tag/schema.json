{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "tags",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "typ": {
      "type": "enumeration",
      "enum": [
        "city",
        "countie",
        "service"
      ],
      "default": "city",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 160
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "lead": {
      "type": "component",
      "repeatable": false,
      "component": "content-parts.lead",
      "required": true
    },
    "contentparts": {
      "type": "dynamiczone",
      "components": [
        "content-parts.txt"
      ],
      "required": true
    },
    "views": {
      "type": "component",
      "repeatable": false,
      "component": "stats.views"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "tags"
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::client.client",
      "inversedBy": "counties"
    }
  }
}
